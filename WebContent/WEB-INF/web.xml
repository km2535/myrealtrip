<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>myrealtrip</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
   
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
	<servlet-name>IndexFrontController</servlet-name> 
	<servlet-class>app.card.IndexFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexFrontController</servlet-name>
  	<url-pattern>/index.jsp</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>UserFrontController</servlet-name> 
	<servlet-class>app.user.UserFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserFrontController</servlet-name>
  	<url-pattern>*.us</url-pattern>
  </servlet-mapping>

 <servlet>
	<servlet-name>AirFrontController</servlet-name> 
	<servlet-class>app.air.AirFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>AirFrontController</servlet-name>
  	<url-pattern>*.ar</url-pattern>
  </servlet-mapping>

 <servlet>
	<servlet-name>TourFrontController</servlet-name> 
	<servlet-class>app.tour.TourFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>TourFrontController</servlet-name>
  	<url-pattern>*.tr</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>HotelFrontController</servlet-name> 
	<servlet-class>app.hotels.HotelFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>HotelFrontController</servlet-name>
  	<url-pattern>*.ht</url-pattern>
  </servlet-mapping> 
  
  <servlet>
	<servlet-name>ReserveFrontController</servlet-name> 
	<servlet-class>app.loading.ReserveFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>ReserveFrontController</servlet-name>
  	<url-pattern>*.rs</url-pattern>
  </servlet-mapping> 
  
  <servlet>
	<servlet-name>payFrontController</servlet-name> 
	<servlet-class>app.pay.payFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>payFrontController</servlet-name>
  	<url-pattern>*.pm</url-pattern>
  </servlet-mapping> 
  
  <servlet>
	<servlet-name>MytripFrontController</servlet-name> 
	<servlet-class>app.mytrip.MytripFrontController</servlet-class> 	
  </servlet>
  <servlet-mapping>
  	<servlet-name>MytripFrontController</servlet-name>
  	<url-pattern>*.mt</url-pattern>
  </servlet-mapping> 




</web-app>